eRoot":"src/components","sourcesContent":["<template>\n\n  <section id=\"synthese\">\n      <h2>SYNTHESE DE VOTRE COMPTE</h2>\n    <div>\n    <table class=\"table table-striped\">\n    <thead>\n      <tr>\n        <th>id</th>\n        <th>Date</th>\n        <th>Origine</th>\n        <th>Catégorie</th>\n        <th>Débit</th>\n        <th>Crédit</th>\n        <th>Supprimer</th>\n      </tr>\n    </thead> \n    <tbody>\n      <tr v-for=\"mouv of mouvs\" :key=\"mouv.id\">\n        <td>{{mouv.id}}</td>\n        <td>{{mouv.date}}</td>\n        <td>{{mouv.origine}}</td>\n        <td>{{mouv.categorie}}</td>\n        <td>{{mouv.debit}}</td>\n        <td>{{mouv.credit}}</td>\n        <td>\n          <button type=\"button\" class=\"btn btn-link\" @click=\"onclick(mouv.id)\">\n            <i class=\"far fa-trash-alt deleteIcon\"></i>\n          </button>\n        </td>\n      </tr>\n    </tbody>\n     <tfoot class=\"ligneTotal\">\n       <tr>\n         <th>Total</th>\n            <td>{{totalCount}} €</td>\n       </tr>\n     </tfoot>\n\n  </table>\n  </div>\n  </section>\n</template>\n\n<script>\n// import addmouv from \"./addmouv.vue\";\n// import {HTTP} from '../http-constants'\n\n// HTTP.get(...).then(...).catch(...)\n\nexport default {\n  name: \"accounttable\",\n  components: {\n  },\n  props: {\n    // addmouv\n    mouvs: {\n      type: Array,\n      required: true\n    }\n  },\n  computed: {\n    totalCount: function() {\n      let totalC = 0;\n      let totalD = 0;\n      let total = 0;\n      for(let i = 0; i < this.mouvs.length; i++){\n        totalC += parseInt(this.mouvs[i].credit);\n        totalD += parseInt(this.mouvs[i].debit);\n        total = totalC-totalD\n      }       \n      return total;\n    }\n  },\n\n  methods:  {\n    onclick(id) {\n      this.$emit(\"onclick\", id)\n    },\n        \n  }\n};\n</script>\n\n<style>\n#synthese{\n  /* border:2px solid red; */\n  margin-top: 20px;\n}\n.deleteIcon{\n  color: #949494;\n}\n.deleteIcon:hover{\n  color: #ef1e43;\n}\n\n</style>\n"]}]}